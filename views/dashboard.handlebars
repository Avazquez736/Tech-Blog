{{#each userPosts as |userPost|}}
{{>dashCard}}
{{/each}}

<h2>New Post</h2>
<form id="new-post">
    <div>
        <label for="post-title">Title:</label>
        <br>
        <input type="text" id="post-title">
        <br>
    </div>
    <div>
        <label for="post-text">Text:</label>
        <br>
        <textarea name="post-text" id="post-text" cols="40" rows="15"></textarea>
        <br>
    </div>
    <div>
        <button type="submit">Enter</button>
    </div>
</form>

<script src="../js/post.js"></script>